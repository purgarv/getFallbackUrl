{

  "manifest_version": 2,
  "name": "Fallbackurl",
  "version": "1.0",

  "description": "copy video fallback url from reddit",

  "icons": {
    "32": "icons/fb_32x32.png"
  },

  "browser_action": {
    "default_icon": {
      "32": "icons/fb_32x32.png"
    },
    "default_title": "FallbackUrl"
  },

  "background" : {
    "scripts" : ["background.js"]
  },
  "permissions": ["activeTab", "clipboardWrite"]
}